<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aicogestnew.Views.Vendors_Documents.VendorAlbaranes"
             IconImageSource="icon_feed.png"
             Title="Albaranes">
              <ContentPage.Content>    
            <Grid VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Image Source="fondo.jpg}" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" Aspect="Fill" Opacity="75" />
               <StackLayout Spacing="20" Padding="15">
                <Label Text="LISTA DE LAS ALBARANES DISPONIBLES" FontSize="Medium" />               
            </StackLayout>
        </Grid>
    </ContentPage.Content>
</ContentPage>